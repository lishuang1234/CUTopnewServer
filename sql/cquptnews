/*Table structure for table `medianews` */

DROP TABLE IF EXISTS `medianews`;

CREATE TABLE `medianews` (
  `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
  `title` varchar(50) NOT NULL,
  `sourceUrl` varchar(100) NOT NULL,
  `publishTime` varchar(20) NOT NULL,
  `clicks` varchar(50) NOT NULL,
  `picOneUrl` varchar(100) DEFAULT 'null',
  `picTwoUrl` varchar(100) DEFAULT 'null',
  `picThreeUrl` varchar(100) DEFAULT 'null',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=247 DEFAULT CHARSET=utf8;

