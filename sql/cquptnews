/*Table structure for table `weather` */

DROP TABLE IF EXISTS `weather`;

CREATE TABLE `weather` (
  `id` int(20) NOT NULL AUTO_INCREMENT,
  `city` varchar(20) NOT NULL,
  `updateTime` varchar(20) DEFAULT NULL,
  `temper` varchar(20) DEFAULT NULL,
  `wind` varchar(20) DEFAULT NULL,
  `moisture` varchar(20) DEFAULT NULL,
  `dressing` varchar(100) DEFAULT NULL,
  `sports` varchar(100) DEFAULT NULL,
  `oneDay` varchar(100) DEFAULT NULL,
  `twoDay` varchar(100) DEFAULT NULL,
  `threeDay` varchar(100) DEFAULT NULL,
  `fourDay` varchar(100) DEFAULT NULL,
  `fiveDay` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id`)
